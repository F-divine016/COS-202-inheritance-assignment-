Animal.java

public class Animal {
    public void makeSound() {
        System.out.println("The animal makes a sound.");
    }
}


Cat.java

public class Cat extends Animal {
    @Override
    public void makeSound() {
        System.out.println("The cat barks."); 
    }
}


Main.java

public class Main {
    public static void main(String[] args) {
        Animal myAnimal = new Animal();
        Cat myCat = new Cat();

        myAnimal.makeSound(); 
        myCat.makeSound(); 
    }
}
